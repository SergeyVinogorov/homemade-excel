{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },

  "rules": {
    "semi": "off",
    "arrow-parens": "off",
    "comma-dangle": "off",
    "require-jsdoc": "off",
    "linebreak-style": ["error", "windows"],
    "quotes": [
      2,
      "double",
      {
        "avoidEscape": true
      }
    ]
  },
  "extends": ["eslint:recommended", "google"]
}
